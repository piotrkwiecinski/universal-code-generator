{
  "chmod": "0755",
  "directories": [
    "src",
    "templates",
    "config"
  ],
  "finder": [
    {
      "name": "*.php",
      "in": "vendor",
      "exclude": ["kherge", "Tests"]
    }
  ],
  "git-version": "package_version",
  "main": "bin/<command-name>",
  "output": "<command-name>.phar",
  "stub": true
}